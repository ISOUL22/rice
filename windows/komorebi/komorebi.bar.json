{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.37/schema.bar.json",
  "monitor": 0,
  "height": 35,
  "padding": 5,
  "transparency_alpha": 240,
  "font_family": "JetBrains Mono",
  "theme": {
    "palette": "Catppuccin",
    "name": "Mocha",
    "accent": "Mauve"
  },
  "grouping": {
    "kind": "Widget",
    "rounding": 12,
    "style": "Default"
  },
  "left_widgets": [
    {
      "Komorebi": {
        "workspaces": {
          "enable": true,
          "hide_empty_workspaces": false
        },
        "layout": {
          "enable": true,
          "display": "Icon"
        },
        "focused_window": {
          "enable": true,
          "show_icon": true
        }
      }
    }
  ],
  "right_widgets": [
    {
      "Media": {
        "enable": true
      }
    }
  ]
}